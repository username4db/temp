    </init-param>
    <init-param>
    	<param-name>parm2</param-name>
    	<param-value>value2</param-value>
    </init-param>
    <async-supported>true</async-supported>
    <multipart-config>
    	<location>c:\\temp</location></multipart-config>
  </servlet>
  <servlet-mapping>
    <servlet-name>DemoServlet</servlet-name>
    <url-pattern>/DemoServlet</url-pattern>
  </servlet-mapping>
